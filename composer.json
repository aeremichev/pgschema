{
    "name": "racklin/pgschema",
    "description": "postgresql schemas for laravel 7",
    "keywords" : ["laravel", "postgres"],
    "license" : "MIT",
    "authors": [
        {
            "name": "Rack Lin",
            "email": "racklin@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "7.*",
        "illuminate/console": "7.*",
        "illuminate/database": "7.*"
    },
    "autoload": {
        "psr-0": {
            "Racklin\\PGSchema": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Racklin\\PGSchema\\PGSchemaServiceProvider"
            ],
            "aliases": {
                "PGSchema": "Racklin\\PGSchema\\Facades\\PGSchema"
            }
        }
    },
    "minimum-stability": "stable"
}
